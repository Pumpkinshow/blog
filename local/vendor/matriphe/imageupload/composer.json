{  
  "name":"matriphe/imageupload",
  "description":"Upload image using Laravel's build in function and resize it automatically.",
  "keywords":[  
    "laravel",
    "image",
    "upload",
    "intervention",
    "imagine"
  ],
  "type":"library",
  "license":"MIT",
  "authors":[  
    {  
      "name":"Muhammad Zamroni",
      "email":"halo@matriphe.com",
      "homepage":"http://matriphe.com"
    }
  ],
  "require":{
    "php": ">=5.5.9",
    "intervention/image": "^2.4",
    "laravel/framework": "~5.2"
  },
  "require-dev":{  
    "phpunit/phpunit":"~4.0",
    "mockery/mockery":"^0.9.9",
    "orchestra/testbench":"~3.2",
    "orchestra/database":"~3.2"
  },
  "suggest":{  

  },
  "autoload":{  
    "psr-4":{  
      "Matriphe\\Imageupload\\":"src/"
    }
  },
  "extra":{  
    "laravel":{  
      "providers":[  
        "Matriphe\\Imageupload\\ImageuploadServiceProvider"
      ]
    }
  },
  "config":{  
    "sort-packages":true
  },
  "minimum-stability":"dev",
  "prefer-stable":true
}
